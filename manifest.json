{
    "manifest_version": 3,
    "name": "Not-oModerator",
    "version": "1.0",
    "description": "A Chrome extension for users of old reddit that removes AutoModerator comments.",
    "action": {
      "default_popup": "",
      "default_icon": {
        "128": "images/second.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.reddit.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "128": "images/second.png"
    }
  }